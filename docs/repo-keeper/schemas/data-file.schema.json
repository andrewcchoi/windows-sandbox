{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Data File",
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^\\d+\\.\\d+\\.\\d+$",
      "description": "Semantic version"
    },
    "description": {
      "type": "string",
      "minLength": 10,
      "description": "File purpose description"
    }
  }
}
