{
  "name": "sandbox-plugin",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "yzhang.markdown-all-in-one"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "python.defaultInterpreterPath": "/usr/bin/python3",
        "[markdown]": {
          "editor.defaultFormatter": "yzhang.markdown-all-in-one"
        }
      }
    }
  },
  "remoteUser": "node",
  "postCreateCommand": "echo 'âœ“ Sandbox Plugin development environment ready!'",

  // Generated using windows-sandbox plugin Intermediate mode
  // Detected: Documentation/Template repository
  // Languages: Python 3.12 + Node.js 20 (for examples and templates)
  // Services: PostgreSQL, Redis, RabbitMQ (for testing examples)
  "features": {}
}
